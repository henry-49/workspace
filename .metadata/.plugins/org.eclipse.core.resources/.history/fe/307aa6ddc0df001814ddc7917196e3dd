package algorithms;

public class Search {

	public static int linearSearch(int[] dataSet, int target) {
		// Iterate through dataSet searching for target
		for (int i = 0; i < dataSet.length; i++) {
			if( dataSet[i] == target) {
				return i;
			}else {
				System.out.println("No match data fount");
			}
		}
		return -1;
		
	}
}
