package datastucture;

import java.util.*;


public class Maps {

	public static void main(String[] args) {
		// Implement map interface
		// TreeMap organize with key order  
		Map<Integer, String> hMap = new TreeMap<Integer, String>();
		mapUtil(hMap);
	}
	
	public static void mapUtil(Map<Integer, String> map) {
		// Add key-value pairs
		map.put(101, "Jason");
		map.put(501, "Mark");
		map.put(33, "Willson");
		map.put(103, "Sharon");
		map.put(73, "Kelly");
		
		// 2. Expose / access elements
		System.out.println("see all the keys: "+ map.keySet());
		System.out.println("see all key-value pairs: "+map);
		
		// 3. Iterate through the key-value pairs
		for(int key :map.keySet()) {
			System.out.println(key);
		}
	}

}
